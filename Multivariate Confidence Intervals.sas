/* Getting a covariance matrix, correlation matrix and mean vector in SAS */

PROC CORR DATA = hsb COV NOPROB;
VAR rdg wrtg math sci civ;
by sex;
RUN;

/* Getting just the correlation matrix */

PROC CORR DATA = hsb NOSIMPLE NOPROB;
VAR rdg wrtg math sci civ;
by sex;
RUN;


/* or if you want to use the proc iml to get the sample statistics 
then don't use proc corr but use the following code */

proc iml;
/* A module that compute the mean vector, SSCP matrix, covariance
   matrix and correlation matrix of an (n x p) matrix of observations */
start samplestats(X,Xbar,W,S,R,n);
  n = nrow(X);
  one = J(n,1);
  Xbar = X`*one/n;
  W =  (X - one*Xbar`)` * (X - one*Xbar`);
  S = W/(n-1);
  Dsqrt = sqrt(diag(S));
  R = inv(Dsqrt)*S*inv(Dsqrt);
Finish samplestats;
/* Read data set into two matrices */
use work.hsb;
read all var{ RDG WRTG MATH SCI CIV} where(SEX=1) into Xbt;
read all var{ RDG WRTG MATH SCI CIV} where(SEX=2) into Xgt;
/* Compute sample statistics */
run samplestats(Xbt,Xbtbar,WXbt,SXbt,RXbt,nXbt);
run samplestats(Xgt,Xgtbar,WXgt,SXgt,RXgt,nXgt);
/*xbar1*/
print Xbtbar 
      Xgtbar;

/* Information Given */
p=2;
n1=nXbt;
n2=nXgt;

xbar1=Xbtbar;

xbar2=Xgtbar;

s1=SXbt;

s2=SXgt;

/* Computations for testing mu1-mu2=0 */

Spool = ( (n1-1)*s1 + (n2-1)*s2 )/ ( n1 + n2 - 2 );
Sdiff = (1/n1 + 1/n2)*Spool;

T2 = (xbar1-xbar2)`*inv(Sdiff)*(xbar1-xbar2);


dfden = n1+n2-p-1;
k = (n1+n2-2)*p/dfden;
alpha=.01;
prob=1-alpha;
F_1_alpha = quantile('F',prob,p,dfden);
Fcrit = k * F_1_alpha;
Fstat = T2/k;
pvalue = 1 - cdf('F',Fstat,p,dfden);
print n1 n2,
      xbar1 xbar2 ,
      Spool,
      Sdiff,
      T2 p dfden F_1_alpha Fcrit,
	  Fstat pvalue;

/* 100(1-alpha)% Confidence Region */
call eigen(lambda,E,Spool);
print lambda E;
diff = xbar1-xbar2;
major1 = diff + sqrt(lambda[1,1])*sqrt((1/n1 + 1/n2)*Fcrit)*E[,1];
major2 = diff - sqrt(lambda[1,1])*sqrt((1/n1 + 1/n2)*Fcrit)*E[,1];
minor1 = diff + sqrt(lambda[2,1])*sqrt((1/n1 + 1/n2)*Fcrit)*E[,2];
minor2 = diff - sqrt(lambda[2,1])*sqrt((1/n1 + 1/n2)*Fcrit)*E[,2];




print diff major1 major2 minor1 minor2;


/******************************************/

/* Simultaneous T2 intervals */

T2_1upper = diff[1,1] + sqrt(Fcrit)*sqrt( (1/n1 + 1/n2)*Spool[1,1] );
T2_1lower = diff[1,1] - sqrt(Fcrit)*sqrt( (1/n1 + 1/n2)*Spool[1,1] );

T2_2upper = diff[2,1] + sqrt(Fcrit)*sqrt( (1/n1 + 1/n2)*Spool[2,2] );
T2_2lower = diff[2,1] - sqrt(Fcrit)*sqrt( (1/n1 + 1/n2)*Spool[2,2] );

print T2_1lower T2_1upper,
      T2_2lower T2_2upper;
      
      
      
      







